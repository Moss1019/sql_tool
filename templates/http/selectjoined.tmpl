export function get{tablenamepascal}sFor{parenttablenamepascal}({parentcolcamel}: number, onSuccess: (data: any) => void, onError: (err: any) => void) {
	const url = `${SERVER_END_POINT}/api/{tablenamelower}s/for{parenttablenamepascal}/${{parentcolcamel}}`;
	return axios.get(url)
		.then(result => {
			onSuccess(result);
		})
		.catch(err => {
			onError(err);
		});
};
